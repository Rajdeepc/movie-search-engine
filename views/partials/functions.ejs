<%
minutesToHours = function(Minutes) {
  var hours = Math.floor(Minutes / 60);
  var minutes = Minutes % 60;
  if (hours === 0)
    return Minutes + " minutes";
  else if (minutes == 0)
    return hours.toString() + "hr ";
  else
    return hours.toString() + "hr " + minutes.toString() + " minutes";
}
%>

